amethyst Move_Assignments2 < Traverser_Clone2 {
	itrans=root	

	root=traverse 
	
	visit = Bind[@name:name traverse[.* @expr:expr  {expr}=>move(name) ]]

	move(name) = Or[ move(name)*:ary {Or[{:ary=>ary}]} ]
								| Seq[ (:el (&eof {el}=>move(name) | {el}) )*:ary  {Seq[{:ary=>ary}]} ] 
								| Pass[@var:var @to=>move(name):to @enter:enter] @Pass
								| anything:{_Bind(name,it,false)}
}


