O=""
comp(){
echo $1
git checkout $1.rb
git checkout $1_c.c
gcc -I. -I/usr/lib/ruby/1.8/x86_64-linux -I/usr/lib/ruby/1.8/x86_64-linux -I.   -fPIC -fno-strict-aliasing -g -g $O  -fPIC   -c $1_c.c
gcc -shared -o $1_c.so $1_c.o -L. -L/usr/lib -L.  -rdynamic -Wl,-export-dynamic    -lruby1.8  -lpthread -lrt -ldl -lcrypt -lm   -lc
}
for I in *.rb ;do
comp `basename $I .rb`
done
